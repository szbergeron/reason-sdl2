{
  "name": "reason-sdl2",
  "version": "2.10.3005",
  "description": "SDL2 bindings for OCaml",
  "license": "MIT",
  "scripts": {
    "format": "esy dune build @fmt --auto-promote",
    "run": "esy x test_sdl2"
  },
  "esy": {
    "build": [
      [
        "dune",
        "build",
        "--root",
        "."
      ],
      [
        "dune",
        "build",
        "--root",
        ".",
        "./bin_js/test_sdl2.bc.js"
      ]
    ],
    "install": [
      "esy-installer"
    ]
  },
  "dependencies": {
    "@opam/dune": "^1.7.3",
    "@esy-ocaml/reason": "^3.3.7",
    "refmterr": "^3.1.0",
    "@opam/js_of_ocaml": "^3.3.0",
    "@opam/js_of_ocaml-compiler": "^3.3.0",
    "@opam/js_of_ocaml-lwt": "^3.3.0",
    "@opam/lwt": "^4.0.0",
    "@opam/lwt_ppx": "^1.1.0",
    "esy-sdl2": "^2.0.10004",
    "esy-cmake": "^0.3.4",
    "reason-gl-matrix": "^0.9.9304"
  },
  "resolutions": {
    "@opam/cmdliner": "1.0.2"
  },
  "devDependencies": {
    "ocaml": ">=4.6.0 <4.9.0"
  }
}
